# Cache and logs (Symfony2)
/backend/app/cache/*
/backend/app/logs/*
!app/cache/.gitkeep
!app/logs/.gitkeep

# Email spool folder
/backend/app/spool/*

# Cache, session files and logs (Symfony3)
/backend/var/cache/*
/backend/var/logs/*
/backend/var/sessions/*
!var/cache/.gitkeep
!var/logs/.gitkeep
!var/sessions/.gitkeep

# Logs (Symfony4)
/backend/var/log/*
!var/log/.gitkeep

# Parameters
/backend/app/config/parameters.yml
/backend/app/config/parameters.ini

# Managed by Composer
/backend/app/bootstrap.php.cache
/backend/var/bootstrap.php.cache
/backend/bin/*
!bin/console
!bin/symfony_requirements
/backend/vendor/

# Assets and user uploads
/backend/web/bundles/
/backend/web/uploads/

# PHPUnit
/backend/app/phpunit.xml
/backend/phpunit.xml

# Build data
/backend/build/

# Composer PHAR
/backend/composer.phar

# Backup entities generated with doctrine:generate:entities command
**/Entity/*~

# Embedded web-server pid file
/.web-server-pid
